@model IEnumerable<NZWalks.UI.Models.DTO.RegionDto>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h1 class="mt-3">Regions</h1>

<table class="table table-bordered">
    <thead>
    <tr>
        <th>Code</th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var walk in Model)
    {
        <tr>
            <td>@walk.Code</td>
            <td>@walk.Name</td>
        </tr>
    }
    </tbody>
</table>